{
  "name": "vue-js-star-rating",
  "version": "3.0.1",
  "description": "Zero-dependency Vue 3.5+ star rating component â€” WCAG 2.2 accessible, TypeScript-first, half-star precision, Lucide/FontAwesome/custom icons",
  "author": "Pooya Golchian <pooya.golchian@gmail.com>",
  "license": "MIT",
  "type": "module",
  "main": "./dist/vue-star-rate.umd.cjs",
  "module": "./dist/vue-star-rate.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/vue-star-rate.js",
      "require": "./dist/vue-star-rate.umd.cjs"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx --fix",
    "typecheck": "vue-tsc --noEmit",
    "prepublishOnly": "npm run build",
    "docs:dev": "cd docs && pnpm run dev",
    "docs:build": "cd docs && pnpm run build",
    "docs:preview": "cd docs && pnpm run preview"
  },
  "dependencies": {},
  "peerDependencies": {
    "vue": "^3.5.0"
  },
  "peerDependenciesMeta": {
    "@fortawesome/fontawesome-free": {
      "optional": true
    },
    "lucide-vue-next": {
      "optional": true
    }
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.7.0",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^5.2.0",
