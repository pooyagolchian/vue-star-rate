{
  "name": "vue-js-star-rating",
  "version": "3.0.0",
  "description": "A highly customizable star rating component for Vue 3 with Lucide, FontAwesome, and custom icon support",
  "author": "Pooya Golchian <pooya.golchian@gmail.com>",
  "license": "MIT",
  "type": "module",
  "main": "./dist/vue-star-rate.umd.cjs",
  "module": "./dist/vue-star-rate.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/vue-star-rate.js",
      "require": "./dist/vue-star-rate.umd.cjs"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx --fix",
    "typecheck": "vue-tsc --noEmit",
    "prepublishOnly": "npm run build",
    "docs:dev": "cd docs && npx vite",
    "docs:build": "cd docs && npx vite build",
    "docs:preview": "cd docs && npx vite preview"
  },
  "dependencies": {},
  "peerDependencies": {
    "vue": "^3.5.0"
  },
  "peerDependenciesMeta": {
    "@fortawesome/fontawesome-free": {
      "optional": true
    },
    "lucide-vue-next": {
      "optional": true
    }
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.7.0",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^5.2.0",
    "@vitest/coverage-v8": "^2.1.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.0.0",
    "eslint-plugin-vue": "^9.32.0",
    "happy-dom": "^14.0.0",
    "lucide-vue-next": "^0.469.0",
    "typescript": "~5.7.0",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.0.0",
    "vitest": "^2.1.0",
    "vue": "^3.5.0",
    "vue-tsc": "^2.1.0"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pooyagolchian/vue-star-rate.git"
  },
  "bugs": {
    "url": "https://github.com/pooyagolchian/vue-star-rate/issues"
  },
  "homepage": "https://github.com/pooyagolchian/vue-star-rate#readme",
  "keywords": [
    "vue",
    "vue3",
    "vue-3",
    "star-rating",
    "rating",
    "stars",
    "vue-component",
    "fontawesome",
    "lucide",
    "typescript",
    "accessible",
    "customizable",
    "half-stars",
    "rating-component"
  ]
}
