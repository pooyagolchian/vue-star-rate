    "eslint": "^9.0.0",
    "eslint-plugin-vue": "^9.32.0",
    "happy-dom": "^14.0.0",
    "lucide-vue-next": "^0.469.0",
    "typescript": "~5.7.0",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.0.0",
    "vitest": "^2.1.0",
    "vue": "^3.5.0",
    "vue-tsc": "^2.1.0"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pooyagolchian/vue-star-rate.git"
