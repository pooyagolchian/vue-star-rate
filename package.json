    "vue-plugin",
    "fontawesome",
    "lucide",
    "typescript",
    "accessible",
    "a11y",
    "wcag",
    "wcag2",
    "aria",
    "customizable",
    "v-model",
    "composition-api",
    "define-model"
  ]
